# Configurações do servidor
NODE_ENV=development
PORT=3000

# Diretório de downloads
DOWNLOAD_DIR=/app/downloads

# Tempo máximo de vida dos arquivos em milissegundos (padrão: 1 hora)
# 3600000 = 1 hora
# 7200000 = 2 horas
# 86400000 = 24 horas
FILE_MAX_AGE_MS=3600000

# Timeout para download do vídeo em milissegundos (padrão: 10 minutos)
# 300000 = 5 minutos
# 600000 = 10 minutos
# 1800000 = 30 minutos
DOWNLOAD_TIMEOUT_MS=600000

# Tamanho máximo do vídeo em MB (padrão: 500MB)
MAX_VIDEO_SIZE_MB=500

# Duração máxima do vídeo em segundos (padrão: 1 hora)
# 1800 = 30 minutos
# 3600 = 1 hora
# 7200 = 2 horas
MAX_VIDEO_DURATION_SEC=3600
